{
    "name": "笔趣阁",
    "url": "www.biquge.info",
    "version": 100,
    "search": {
        "url": "http://www.biquge.info/modules/article/search.php?searchkey=${key}",
        "list": "tr:nth-child(n+2)",
        "name": "td:nth-child(1)",
        "author": "td:nth-child(3)",
        "detail": "td:nth-child(1) > a"
    },
    "detail": {
        "cover": "#fmimg > img",
        "summary": "#intro > p:nth-child(1)",
        "update": "#info > p:nth-child(4)@match->(?<=最后更新 :).+"
    },
    "catalog": {
        "list": "#list > dl > dd",
        "name": "a",
        "chapter": "a"
    },
    "chapter": {
        "content": "#content"
    }
}
