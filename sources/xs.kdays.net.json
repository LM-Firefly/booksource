{
    "name": "萌文库",
    "url": "xs.kdays.net",
    "version": 100,
    "search": {
        "url": "http://xs.kdays.net/search/name?w=${key}",
        "list": ".book",
        "name": "h3",
        "author": "p:nth-child(3)@match->(?<=作者:).+",
        "cover": "img",
        "detail": "h3 > a"
    },
    "detail": {
        "summary": "blockquote",
        "lastChapter": ".last-chapter > a@match->(?<=最新章节:).+",
        "catalog": ".read-btn"    
    },
    "catalog": {
        "list": ".info-side > ul > li",
        "name": "a",
        "chapter": "a"
    },
    "chapter": {
        "content": "article > p"
    },
