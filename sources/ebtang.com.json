            "size": 100,
            "categories": [{"key": "week", "value": "日"}, {"key": "month", "value": "月"}, {"key": "all", "value": "全"}],
            "list":"$.rankList[*]",
            "name":"$.name",
            "author":"$.authorNick",
            "summary":"$.shotInfo",
