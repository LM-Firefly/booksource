{
    "name": "雁北堂",
    "url": "ebtang.com",
    "version": 102,
    "search": {
        "url": "http://ebtang.com/book/search?searchName=${key}",  
        "charset": "UTF-8",
        "list": "ul > li",
        "name": "li@attr->d-name",
        "author": "li@attr->d-nick",
        "cover": "li@attr->d-cover",
        "detail": "http://www.ebtang.com/book/${li@attr->d-id}"
    },
    "detail": {
        "summary": "meta[property=og:description]@attr->content",
        "status": "span.status",
        "category":"meta[property=og:novel:category]@attr->content",
        "words":"#bookDetail@attr->d-words",
        "update": "#bookDetail@attr->d-lasttime",
        "lastChapter": "#bookDetail@attr->d-lasttitle",
        "catalog": "/book/${#bookDetail@attr->d-id}/directory?idd=${#bookDetail@attr->d-id}"
    },
    "catalog": {
        "list": "b",
        "name": "b@attr->d-title",
        "chapter": "http://m.ebtang.com/m/book/readbook/${$params.idd}/${b@attr->d-id}"
    },
    "chapter": {
        "content":"$..content"
    },
    "rank": [
        {
            "title":"点击榜",
            "url": "http://m.ebtang.com/m/rank_new/click?randomString=&type=${key}",
            "size": 100,
            "categories": [{"key": "week", "value": "日"}, {"key": "month", "value": "月"}, {"key": "all", "value": "全"}],
            "list":"$.rankList[*]",
            "name":"$.name",
            "author":"$.authorNick",
            "summary":"$.shotInfo",
            "cover":"$.bigCoverImage",
            "detail":"http://www.ebtang.com/book/${$.id}"

        },{
            "title":"打赏榜",
            "url": "http://m.ebtang.com/m/rank_new/reward?randomString=&type=${key}",
            "size": 100,
            "categories": [{"key": "week", "value": "日"}, {"key": "month", "value": "月"}, {"key": "all", "value": "全"}],
            "list":"$.rankList[*]",
            "name":"$.name",
            "author":"$.authorNick",
            "summary":"$.shotInfo",
            "cover":"$.bigCoverImage",
            "detail":"http://www.ebtang.com/book/${$.id}"

        },{
            "title":"金票榜",
            "url": "http://m.ebtang.com/m/rank_new/golden?randomString=&type=${key}",
            "size": 100,
            "categories": [{"key": "week", "value": "日"}, {"key": "month", "value": "月"}, {"key": "all", "value": "全"}],
            "list":"$.rankList[*]",
            "name":"$.name",
            "author":"$.authorNick",
            "summary":"$.shotInfo",
            "cover":"$.bigCoverImage",
            "detail":"http://www.ebtang.com/book/${$.id}"

        },{
            "title":"订阅榜",
            "url": "http://m.ebtang.com/m/rank_new/rss?randomString=&type=${key}",
            "size": 100,
            "categories": [{"key": "week", "value": "日"}, {"key": "month", "value": "月"}, {"key": "all", "value": "全"}],
            "list":"$.rankList[*]",
            "name":"$.name",
            "author":"$.authorNick",
            "summary":"$.shotInfo",
            "cover":"$.bigCoverImage",
            "detail":"http://www.ebtang.com/book/${$.id}"

        },{
            "title":"新书榜",
            "url": "http://m.ebtang.com/m/rank_new/newBook?randomString=&type=${key}",
            "size": 100,
            "categories": [{"key": "week", "value": "日"}, {"key": "month", "value": "月"}, {"key": "all", "value": "全"}],
            "list":"$.rankList[*]",
            "name":"$.name",
