{
    "name": "百合会",
    "url": "www.yamibo.com",
    "version": 101,
    "search": {
        "url": "https://m.yamibo.com/api/search-novel?k=${key}",
        "list": "$.lists[*]",
        "name": "$.workTitle",
        "author": "$.author",
        "cover": "$.cover",
        "summary": "$.workIntro",
        "detail": "https://m.yamibo.com/novel/${$.workId}"
    },
    "detail": {
        "status": "div:nth-child(8) > div:nth-child(2) > div:nth-child(2)@match->(?<=作品状态：).+",
        "update": "div:nth-child(8) > div:nth-child(4) > div:nth-child(1)@match->(?<=更新时间：).+",
        "lastChapter": ".work-nChpt@match->(?<=最新：).+"
    },
    "catalog": {
        "list": ".modal-content > a",
        "name": ".weui-cell__bd > p",
        "chapter": "a"
    },
    "chapter": {
        "content": "#content > p"
    },
