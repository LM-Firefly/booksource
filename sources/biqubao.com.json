{
    "name": "笔趣宝",
    "url": "www.biqubao.com",
    "version": 100,
    "search": {
        "url": "https://www.biqubao.com/search.php?keyword=${key}",
        "charset": "UTF-8",
        "list": ".result-list>div",
        "name": "h3>a>span",
        "author": ".result-game-item-info>p:nth-child(1)>span:nth-child(2)",
        "detail":"h3>a"
    },
    "detail": {
        "cover":"img",
        "status": "#info>p:nth-child(3)@match->(?=：)(.+)(?=,)",
        "update": "#info>p:nth-child(4)@match->(?=最后更新：)",
        "lastChapter": "#info>p:nth-child(5)",
        "summary":"#intro"
        },
    "catalog": {
        "list": "#list>dl>dd",
        "name": "dd>a",
        "chapter": "dd>a"
    },
    "chapter": {
        "content": "#content"
    }
}